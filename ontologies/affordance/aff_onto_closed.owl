<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://test.org/affordance.owl"
         xmlns="http://test.org/affordance.owl#">

<owl:Ontology rdf:about="http://test.org/affordance.owl"/>

<owl:ObjectProperty rdf:about="#quality">
  <rdfs:domain rdf:resource="#KitchenEntity"/>
  <rdfs:range rdf:resource="#Quality"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#color">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:subPropertyOf rdf:resource="#quality"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#location">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:subPropertyOf rdf:resource="#quality"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#form">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:subPropertyOf rdf:resource="#quality"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#implies">
  <rdfs:domain rdf:resource="#Quality"/>
  <rdfs:range rdf:resource="#Affordance"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#affords">
  <rdfs:domain rdf:resource="#KitchenEntity"/>
  <rdfs:range rdf:resource="#Action"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#KitchenEntity">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#quality"/>
      <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#color"/>
      <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#location"/>
      <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#form"/>
      <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#affords"/>
      <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Quality">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Class>
      <owl:oneOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#soft"/>
        <rdf:Description rdf:about="#hard"/>
      </owl:oneOf>
    </owl:Class>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#implies"/>
      <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Affordance">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Class>
      <owl:oneOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#eatable"/>
      </owl:oneOf>
    </owl:Class>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Action">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Class>
      <owl:oneOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#eat"/>
        <rdf:Description rdf:about="#cut"/>
      </owl:oneOf>
    </owl:Class>
  </rdfs:subClassOf>
</owl:Class>

<Action rdf:about="#cut">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Action>

<Action rdf:about="#eat">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Action>

<Quality rdf:about="#soft">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <rdf:type>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#implies"/>
      <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
    </owl:Restriction>
  </rdf:type>
</Quality>

<Quality rdf:about="#hard">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <rdf:type>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#implies"/>
      <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
    </owl:Restriction>
  </rdf:type>
</Quality>

<Affordance rdf:about="#eatable">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Affordance>


</rdf:RDF>
