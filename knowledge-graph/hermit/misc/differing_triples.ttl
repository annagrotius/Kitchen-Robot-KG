@prefix ns1: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://test.org/bft.owl#Coffee> rdfs:subClassOf ns1:Nothing ;
    ns1:equivalentClass <http://test.org/bft.owl#Fork> .

<http://test.org/bft.owl#Condiment> ns1:equivalentClass <http://test.org/bft.owl#Spread> .

<http://test.org/bft.owl#Cutlery> ns1:equivalentClass <http://test.org/bft.owl#Knife> .

<http://test.org/bft.owl#Drink> ns1:equivalentClass <http://test.org/bft.owl#Juice> .

<http://www.semanticweb.org/annag/ontologies/2020/6/untitled-ontology-32> a ns1:Ontology .

<http://test.org/affordance.owl#affords> rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread> ;
    ns1:equivalentProperty <http://test.org/affordance.owl#color> .

<http://test.org/bft.owl#Fork> rdfs:subClassOf ns1:Nothing ;
    ns1:equivalentClass <http://test.org/bft.owl#Milk> .

<http://test.org/bft.owl#Juice> rdfs:subClassOf <http://test.org/affordance.owl#KitchenEntity> .

<http://test.org/bft.owl#Knife> rdfs:subClassOf <http://test.org/bft.owl#Tableware> .

<http://test.org/bft.owl#Milk> rdfs:subClassOf ns1:Nothing ;
    ns1:equivalentClass <http://test.org/bft.owl#Spoon> .

<http://test.org/bft.owl#Spoon> rdfs:subClassOf ns1:Nothing ;
    ns1:equivalentClass <http://test.org/bft.owl#Tea> .

<http://test.org/bft.owl#Spread> rdfs:subClassOf <http://test.org/bft.owl#Food> .

<http://test.org/bft.owl#Tea> rdfs:subClassOf ns1:Nothing ;
    ns1:equivalentClass ns1:Nothing .

ns1:bottomObjectProperty ns1:equivalentProperty [ ns1:equivalentProperty [ ns1:equivalentProperty [ ns1:equivalentProperty [ ns1:equivalentProperty [ ns1:equivalentProperty [ ns1:equivalentProperty [ ns1:equivalentProperty [ ns1:equivalentProperty [ ns1:equivalentProperty [ ns1:inverseOf <http://test.org/bft.owl#used_with> ] ;
                                                                            ns1:inverseOf <http://test.org/bft.owl#spread_on> ] ;
                                                                    ns1:inverseOf <http://test.org/bft.owl#served_with> ] ;
                                                            ns1:inverseOf <http://test.org/bft.owl#can_cut> ] ;
                                                    ns1:inverseOf <http://test.org/affordance.owl#quality> ] ;
                                            ns1:inverseOf <http://test.org/affordance.owl#location> ] ;
                                    ns1:inverseOf <http://test.org/affordance.owl#implies> ] ;
                            ns1:inverseOf <http://test.org/affordance.owl#form> ] ;
                    ns1:inverseOf <http://test.org/affordance.owl#color> ] ;
            ns1:inverseOf <http://test.org/affordance.owl#affords> ] .

<http://test.org/affordance.owl#color> a ns1:DatatypeProperty ;
    rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread>,
        <http://test.org/bft.owl#is_edible>,
        <http://test.org/bft.owl#primary_usage> ;
    ns1:equivalentProperty <http://test.org/affordance.owl#form> .

<http://test.org/affordance.owl#form> a ns1:DatatypeProperty ;
    rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread>,
        <http://test.org/bft.owl#is_edible>,
        <http://test.org/bft.owl#primary_usage> ;
    ns1:equivalentProperty <http://test.org/affordance.owl#implies>,
        <http://test.org/affordance.owl#location> .

<http://test.org/affordance.owl#implies> rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread> ;
    ns1:equivalentProperty <http://test.org/affordance.owl#location> .

<http://test.org/affordance.owl#quality> rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread> ;
    ns1:equivalentProperty <http://test.org/bft.owl#can_cut> .

<http://test.org/bft.owl#can_cut> rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread> ;
    ns1:equivalentProperty <http://test.org/bft.owl#served_with> .

<http://test.org/bft.owl#served_with> rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread> ;
    ns1:equivalentProperty <http://test.org/bft.owl#spread_on> .

<http://test.org/bft.owl#spread_on> rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread> ;
    ns1:equivalentProperty <http://test.org/bft.owl#used_with> .

<http://test.org/bft.owl#used_with> rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread> ;
    ns1:equivalentProperty ns1:bottomObjectProperty .

<http://test.org/affordance.owl#location> a ns1:DatatypeProperty ;
    rdfs:subPropertyOf [ ns1:inverseOf <http://test.org/bft.owl#can_spread> ],
        <http://test.org/bft.owl#can_spread>,
        <http://test.org/bft.owl#is_edible>,
        <http://test.org/bft.owl#primary_usage> ;
    ns1:equivalentProperty <http://test.org/affordance.owl#quality>,
        ns1:bottomDataProperty .

